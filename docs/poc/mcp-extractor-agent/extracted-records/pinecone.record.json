    "extensions": [
        {
            "name": "schema.oasf.agntcy.org/features/runtime/mcp",
            "version": "v1.0.0",
            "data": {
                "servers": {
                    "pinecone": {
                        "command": "docker",
                        "args": [
                            "run",
                            "-i",
                            "--rm",
                            "-e",
                            "PINECONE_API_KEY",
                            "-e",
                            "PINECONE_ENVIRONMENT",
                            "-e",
                            "PINECONE_INDEX",
                            "ghcr.io/pinecone-io/pinecone-mcp-server"
                        ],
                        "env": {
                            "PINECONE_API_KEY": "${input:PINECONE_API_KEY}",
                            "PINECONE_ENVIRONMENT": "${input:PINECONE_ENVIRONMENT}",
                            "PINECONE_INDEX": "${input:PINECONE_INDEX}"
                        }
                    }
                }
            }
        },
        {
            "name": "schema.oasf.agntcy.org/features/runtime/prompt",
            "version": "v1.0.0",
            "data": {
                "prompts": [
                    {
                        "name": "Pinecone Vector DB Assistant",
                        "description": "Pinecone vector database assistant",
                        "prompt": "You are a Pinecone assistant. Help users store, query, and manage vector embeddings for semantic search and similarity operations."
                    }
                ]
            }
        }
    ]
}
