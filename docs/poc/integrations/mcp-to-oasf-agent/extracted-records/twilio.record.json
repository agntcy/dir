{
    "name": "twilio/twilio-mcp-server",
    "version": "1.0.0",
    "description": "Official Twilio MCP server that provides access to Twilio communications APIs for SMS, voice, video, and other messaging services.",
    "authors": [
        "Twilio Inc. <support@twilio.com>"
    ],
    "created_at": "2023-10-25T13:15:00Z",
    "skills": [
        {
            "category_name": "Natural Language Processing",
            "category_uid": 1,
            "class_name": "Text Completion",
            "class_uid": 10201
        },
        {
            "category_name": "Natural Language Processing",
            "category_uid": 1,
            "class_name": "Dialogue Generation",
            "class_uid": 10204
        },
        {
            "category_name": "Natural Language Processing",
            "category_uid": 1,
            "class_name": "Text Summarization",
            "class_uid": 10202
        }
    ],
    "locators": [
        {
            "type": "source-code",
            "url": "https://github.com/twilio/twilio-mcp-server"
        }
    ],
    "extensions": [
        {
            "name": "schema.oasf.agntcy.org/features/runtime/mcp",
            "version": "v1.0.0",
            "data": {
                "servers": {
                    "twilio": {
                        "command": "docker",
                        "args": [
                            "run",
                            "-i",
                            "--rm",
                            "-e",
                            "TWILIO_ACCOUNT_SID",
                            "-e",
                            "TWILIO_AUTH_TOKEN",
                            "ghcr.io/twilio/twilio-mcp-server"
                        ],
                        "env": {
                            "TWILIO_ACCOUNT_SID": "${input:TWILIO_ACCOUNT_SID}",
                            "TWILIO_AUTH_TOKEN": "${input:TWILIO_AUTH_TOKEN}"
                        }
                    }
                }
            }
        },
        {
            "name": "schema.oasf.agntcy.org/features/runtime/prompt",
            "version": "v1.0.0",
            "data": {
                "prompts": [
                    {
                        "name": "Twilio Communications Assistant",
                        "description": "Twilio communications assistant",
                        "prompt": "You are a Twilio assistant. Help users send SMS messages, make phone calls, and implement other Twilio communication services."
                    }
                ]
            }
        }
    ]
}
