{
    "name": "airtable/airtable-mcp-server",
    "version": "1.0.0",
    "description": "Official Airtable MCP server that provides access to Airtable bases, tables, records, and other database functionality.",
    "authors": [
        "Airtable <support@airtable.com>"
    ],
    "created_at": "2023-12-05T16:40:00Z",
    "skills": [
        {
            "category_name": "Natural Language Processing",
            "category_uid": 1,
            "class_name": "Text Completion",
            "class_uid": 10201
        },
        {
            "category_name": "Natural Language Processing",
            "category_uid": 1,
            "class_name": "Question Answering",
            "class_uid": 10302
        },
        {
            "category_name": "Retrieval Augmented Generation",
            "category_uid": 6,
            "class_name": "Document or Database Question Answering",
            "class_uid": 602
        }
    ],
    "locators": [
        {
            "type": "source-code",
            "url": "https://github.com/airtable/airtable-mcp-server"
        }
    ],
    "extensions": [
        {
            "name": "schema.oasf.agntcy.org/features/runtime/mcp",
            "version": "v1.0.0",
            "data": {
                "servers": {
                    "airtable": {
                        "command": "docker",
                        "args": [
                            "run",
                            "-i",
                            "--rm",
                            "-e",
                            "AIRTABLE_API_KEY",
                            "-e",
                            "AIRTABLE_BASE_ID",
                            "ghcr.io/airtable/airtable-mcp-server"
                        ],
                        "env": {
                            "AIRTABLE_API_KEY": "${input:AIRTABLE_API_KEY}",
                            "AIRTABLE_BASE_ID": "${input:AIRTABLE_BASE_ID}"
                        }
                    }
                }
            }
        },
        {
            "name": "schema.oasf.agntcy.org/features/runtime/prompt",
            "version": "v1.0.0",
            "data": {
                "prompts": [
                    {
                        "name": "Airtable Database Assistant",
                        "description": "Airtable database management assistant",
                        "prompt": "You are an Airtable assistant. Help users create and manage bases, tables, fields, and records in their Airtable databases."
                    }
                ]
            }
        }
    ]
}
